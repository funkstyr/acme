{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        ".next/**",
        "build/**",
        "dist/**",
        "lib/**",
        "out/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "docker:compose:up": {
      "cache": false
    },
    "docker:compose:destroy": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
